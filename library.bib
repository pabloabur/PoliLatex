Times Cited:237
Cited References Count:339},
author = {Rudomin, P and Schmidt, R F},
isbn = {0014-4819},
journal = {Experimental Brain Research},
keywords = {action-potential propagation,axo-axonic synapses,central nervous-system,excitatory postsynaptic potentials,gaba,gamma-aminobutyric-acid,gene-related peptide,ii muscle afferents,monoamines,neuropeptides,presynaptic inhibition,primary afferent depolarization,rat posterior pituitary,soleus h-reflex,spinal cord,superficial dorsal horn,volume-transmitted inhibition},
number = {1},
