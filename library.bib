journal = {Journal of Biomechanics},
keywords = {emg,emg amplitude,emg-torque model,identification,model,muscle,optimal sampling rate,torque},
number = {14},
pages = {2690--2698},
title = {{Influence of advanced electromyogram (EMG) amplitude processors on EMG-to-torque estimation during constant-posture, force-varying contractions}},
