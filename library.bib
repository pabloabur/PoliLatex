keywords = {antiganglioside antibody,axonal gbs,c,campylobacter jejuni,d-668,debashish chowdhury,new delhi-,park,r},
number = {5},
pages = {267--277},
title = {{Axonal Guillain-Barre syndrome: a critical review}},
url = {<Go to ISI>://WOS:000168342300001$\backslash$nhttp://onlinelibrary.wiley.com/store/10.1034/j.1600-0404.2001.103005267.x/asset/j.1600-0404.2001.103005267.x.pdf?v=1{\&}t=hv6fjtgh{\&}s=b7665e17c8cdeb7fccd084e9dfce1d0395c880ef},
