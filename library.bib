isbn = {1439-6327 (Electronic) 1439-6319 (Linking)},
journal = {European Journal of Applied Physiology},
keywords = {*Isometric Contraction,*Motor Neurons,*Muscle Strength,*Resistance Training,Adaptation,Adult,Aged,Electromyography,Evoked Potentials,Female,H-Reflex,Humans,Lower Extremity,Male,Middle Aged,Motor,Multiple Sclerosis/physiopathology/*therapy,Muscle,Physiological,Skeletal/*innervation,Time Factors,Torque,Treatment Outcome},
