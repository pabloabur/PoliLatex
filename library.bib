isbn = {0340-1200},
journal = {Biological Cybernetics},
keywords = {automated fitting,compartmental,firing pattern,genetic algorithm,model,multi-objective optimization,neuron,optimization,parameter constraining},
number = {4-5},
pages = {371--379},
title = {{Evaluating automated parameter constraining procedures of neuron models by experimental and surrogate data}},
