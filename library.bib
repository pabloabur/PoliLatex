journal = {Journal of neurophysiology},
keywords = {Adolescent,Adult,Analysis of Variance,Biomechanical Phenomena,Brachial Plexus,Brachial Plexus: pathology,Elbow,Electromyography,Electromyography: methods,Exercise,Female,Humans,Male,Motor Neurons,Motor Neurons: physiology,Muscle,Muscle Contraction,Muscle Contraction: physiology,Skeletal,Skeletal: injuries,Skeletal: physiopathology,Task Performance and Analysis,Time Factors},
