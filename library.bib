journal = {The Journal of physiology},
keywords = {Action Potentials,Action Potentials: physiology,Axons,Axons: physiology,Cyclic Nucleotide-Gated Cation Channels,Cyclic Nucleotide-Gated Cation Channels: physiolog,Electric Stimulation,Humans,Membrane Potentials,Membrane Potentials: physiology,Models,Motor Neurons,Motor Neurons: physiology,Myelin Sheath,Myelin Sheath: physiology,Neurological,Potassium Channels,Potassium Channels: physiology,Sensory Receptor Cells,Sensory Receptor Cells: physiology},
