month = {jan},
number = {1},
pages = {380--7},
pmid = {21084684},
title = {{Common synaptic input to the human hypoglossal motor nucleus.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3023365{\{}{\&}{\}}tool=pmcentrez{\{}{\&}{\}}rendertype=abstract http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3023365{\&}tool=pmcentrez{\&}rendertype=abstract},
