journal = {Journal of applied physiology},
keywords = {Adult,Ankle,Ankle Joint,Ankle Joint: physiology,Ankle: physiology,Female,Humans,Isometric Contraction,Isometric Contraction: physiology,Leg,Leg: physiology,Male,Middle Aged,Motor Neurons,Motor Neurons: physiology,Muscle,Muscle Contraction,Muscle Contraction: physiology,Postural Balance,Postural Balance: physiology,Posture,Posture: physiology,Skeletal,Skeletal: physiology,Young Adult},
