keywords = {80 and over,Adult,Aged,Electromyography,Female,Fingers,Fingers: physiology,Hand,Hand Strength,Hand Strength: physiology,Hand: physiology,Humans,Male,Motor Neurons,Motor Neurons: physiology,Muscle,Muscle Contraction,Muscle Contraction: physiology,Skeletal,Skeletal: physiology},
month = {may},
number = {10},
