keywords = {Adult,Analysis of Variance,Biological,Biomechanics,Cues,Female,Gravitation,Humans,Male,Middle Aged,Models,Physical Stimulation,Postural Balance,Proprioception,Psychomotor Performance,Torque,Vestibular Diseases,Vestibular Diseases: physiopathology,Young Adult,cnyrim2009},
