file = {:home/renato/Documents/Mendeley Desktop/Journal of Neurophysiology/Watanabe et al.{\_}2013.pdf:pdf;:home/renato/Documents/Mendeley Desktop/Journal of Neurophysiology/Watanabe et al.{\_}2013(2).pdf:pdf},
journal = {Journal of Neurophysiology},
month = {sep},
number = {11},
