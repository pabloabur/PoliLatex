issn = {1754-8411},
journal = {Disease models {\{}{\&}{\}} mechanisms},
keywords = {als,drug discovery,hsp,motor,sma,zebrafish},
number = {7},
pages = {799--809},
pmid = {24973750},
title = {{Fishing for causes and cures of motor neuron disorders.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24973750},
volume = {7},
