journal = {Neural Computation},
keywords = {cerebral-cortex,cortical-neurons,in-vivo,neocortical pyramidal neurons,network simulations,shunting inhibition,simple cells,spiking neurons,synaptic input,visual-cortex},
number = {9},
pages = {2146--2210},
title = {{Analytical integrate-and-fire neuron models with conductance-based dynamics for event-driven simulation strategies}},
