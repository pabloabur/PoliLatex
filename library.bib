issn = {1095-9572},
journal = {NeuroImage},
keywords = {Biological Clocks,Biological Clocks: physiology,Brain Mapping,Brain Mapping: methods,Cortical Synchronization,Cortical Synchronization: physiology,Female,Humans,Isometric Contraction,Isometric Contraction: physiology,Male,Middle Aged,Motor Cortex,Motor Cortex: physiology,Muscle,Pyramidal Tracts,Pyramidal Tracts: physiology,Skeletal,Skeletal: innervation,Skeletal: physiology},
