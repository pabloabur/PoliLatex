author = {Lee, R H and Heckman, C J},
isbn = {0022-3077},
journal = {Journal of Neurophysiology},
keywords = {alpha-motoneurones,axonal conduction-velocity,decerebrate cat,freely moving cats,hypoglossal motoneurons,innervating muscle-fibers,medial gastrocnemius,membrane-properties,motor-unit type,plateau potentials},
number = {2},
