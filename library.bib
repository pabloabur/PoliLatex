@inproceedings{Elias2011,
abstract = {During upright stance, the human body oscillates due to the displacement of the center of mass. These oscillations can be measured with ground force plates as sway patterns of the center of pressure (CoP, point of the resultant ground reaction forces) both in anterior-posterior and in medial-lateral directions (Prieto et al., 1996). The measurement of the CoP sway is very useful to assess the human postural control system. Several mathematical models of the postural control system, which stand for sway measures of humans, have been developed from a control engineering point of view (Kohn, 2007, Masani et al., 2006, Maurer and Peterka, 2005, Peterka, 2000). One of the aspects that has not been completely elucidated is the contribution of passive ankle torque to postural control (Maurer and Peterka, 2005). Moreover, none of the models proposed previously included a force feedback mechanism , which is important in the regulation of muscle activity (Rothwell, 1986). Therefore, the aim of this study is to develop a biologically plausible model of the postural control system considering force feedback. In order to evaluate the model, quiet stance was simulated and compared with measured data from able-bodied humans.},
address = {Foz do Igua{\{}{\c{c}}{\}}u},
