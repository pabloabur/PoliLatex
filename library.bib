journal = {Experimental Brain Research},
keywords = {GABAergic inhibition,Intramuscular coherenc,Plasticity,Transcranial magnetic stimulation,Visuomotor learning},
number = {4},
pages = {465--474},
pmid = {21785864},
title = {{The effect of baclofen and diazepam on motor skill acquisition in healthy subjects}},
volume = {213},
year = {2011}
}
