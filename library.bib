author = {van der Linden, B J J J and Koopman, H F J M and Grootenboer, H J and Huijing, P A},
journal = {Journal of Electromyography and Kinesiology},
keywords = {aponeurosis,architecture,fiber,isometric contractions,lateralis muscle,length-force characteristics,models,motor control,muscle geometry,performance,rat gastrocnemius-muscle,tetanic contractions},
number = {2},
