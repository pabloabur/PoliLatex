issn = {0028-0836},
journal = {Nature},
number = {7173},
pages = {1240--1244},
pmid = {18097411},
title = {{Performance variability enables adaptive plasticity of 'crystallized' adult birdsong.}},
volume = {450},
year = {2007}
}
