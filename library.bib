isbn = {0022-3751},
journal = {Journal of Physiology-London},
keywords = {activation,alpha-motoneurones,decerebrate cats,dorsal horn neurons,hindlimb motoneurons,in-vivo,motor units,renshaw cells,short-term plasticity,turtle spinal-cord},
