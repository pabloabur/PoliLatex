keywords = {cat,chronic recording,locomotion,microelectrode array,neurons,neuroprosthesis,sensory neurons,signals},
number = {2},
pages = {240--243},
title = {{Decoding sensory feedback from firing rates of afferent ensembles recorded in cat dorsal root ganglia in normal locomotion}},
