journal = {Cerebral Cortex},
keywords = {EEG,coherence,corticomuscular,force,human},
number = {3},
pages = {628--638},
pmid = {21685397},
title = {{Corticomuscular coherence reflects interindividual differences in the state of the corticomuscular network during low-level static and dynamic forces}},
