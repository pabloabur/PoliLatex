keywords = {Adult,Electric Stimulation,Electromyography,Evoked Potentials,Female,Humans,Male,Motor,Motor Neurons,Motor Neurons: physiology,Motor: physiology,Multivariate Analysis,Muscle,Neural Pathways,Neural Pathways: physiology,Neurophysiological,Neurophysiological: physiology,Posture,Reaction Time,Reaction Time: physiology,Recruitment,Skeletal,Skeletal: innervation,Skeletal: physiology,Synaptic Transmission,Synaptic Transmission: physiology,Young Adult,vieira2012},
mendeley-tags = {vieira2012},
