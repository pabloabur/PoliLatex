journal = {Journal of the Peripheral Nervous System},
keywords = {activity,conduction block,demyelinating neuropathy,fatigue},
number = {3},
pages = {159--168},
pmid = {22003929},
title = {{Activity-dependent conduction failure: Molecular insights}},
volume = {16},
year = {2011}
}
