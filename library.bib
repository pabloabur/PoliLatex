journal = {Journal of Neurophysiology},
keywords = {bistability,calcium currents,cat spinal motoneurons,firing rate,gaba-a,motor cortex,persistent inward currents,plateau potentials,systematic variations,turtle motoneurons},
number = {6},
pages = {3617--3624},
title = {{Active dendritic integration of inhibitory synaptic inputs in vivo}},
