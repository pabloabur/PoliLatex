author = {Baker, S N and Lemon, R N},
isbn = {0022-3077},
journal = {Journal of Neurophysiology},
keywords = {axon collaterals,cervical spinal-cord,conscious monkey,firing rate,hand muscles,human motor units,isometric contractions,orderly recruitment,primate corticomotoneuronal cells,single corticospinal neurons},
number = {3},
pages = {1391--1406},
title = {{Computer simulation of post-spike facilitation in spike-triggered averages of rectified EMG}},
