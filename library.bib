journal = {Experimental Brain Research},
keywords = {activation,arm movements,contractions,coordination,criterion,finger forces,force sharing,hand,human,joint,li1998,load,locomotion,motor redundancy,muscles,skill acquisition,voluntary movement},
language = {English},
