journal = {Journal of Applied Physiology},
keywords = {decerebrate cats,forces consistent,hindlimb motoneurons,human motoneurons,human motor units,human skeletal-muscle,intrinsic activation,plateau potentials,plateau-like behavior,posttetanic potentiation,potentials,recruitment order,rehabilitation,spinal-cord},
