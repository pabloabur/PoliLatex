Cited References Count:31},
author = {Beck, J and Sinderby, C and Weinberg, J and Grassino, A},
isbn = {8750-7587},
journal = {Journal of Applied Physiology},
keywords = {center frequency,distance filtering effects,emg,esophageal,esophageal electrode,lung-volume,position,recordings,spectral-analysis},
number = {3},
pages = {975--985},
title = {{Effects of muscle-to-electrode distance on the human diaphragm electromyogram}},
