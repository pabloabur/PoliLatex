journal = {Journal of the Neurological Sciences},
keywords = {Chronic inflammatory demyelinating Polyneuropathy,Disability,Electrophysiology,Functional,Motor,Rydel-Seiffer,Sensory,Sensory Sum Score},
number = {1-2},
pages = {11--14},
pmid = {20673919},
