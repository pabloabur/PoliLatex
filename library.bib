keywords = {Adult,Female,Humans,Male,Photic Stimulation,Photic Stimulation: methods,Postural Balance,Postural Balance: physiology,Posture,Posture: physiology,Psychomotor Performance,Psychomotor Performance: physiology,creath2005},
mendeley-tags = {creath2005},
month = {mar},
number = {2},
pages = {75--80},
pmid = {15740840},
title = {{A unified view of quiet and perturbed stance: simultaneous co-existing excitable modes.}},
